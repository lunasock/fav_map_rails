<div class="main">
  <div class="container">
    <div class="user-edit-form">
      <%= form_for(@user) do |f| %>
      <p>ユーザーネーム</p>
      <%= f.text_field :user_name %><br>
      <br><p>自己紹介</p>
      <%= f.text_area :user_body, size: "8x8" %><br>
      <br><p>アイコン</p>
      <%= f.attachment_field :icon_image %>
      <br><P>ヘッダー画像</P>
      <%= f.attachment_field :backcover_image %>
      <br><%= f.submit '更新', class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>